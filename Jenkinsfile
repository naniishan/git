pipeline {
   agent any

   stages {
       stage('hello world') {
        steps {
          sh 'cat ishan.txt'
          sh 'touch arima.txt'
} 
}
       
            stage('parallel tasks') {

            parallel {
                 stage('ishan')  
                     sh 'echo "my name is ishan"'

                 stage('aman') 
                       sh 'echo "my name is aman"'


}
}








        stage('sample') {
        steps {
          sh 'cal'
          sh 'echo "my name is ishan"'
          input('do you want to proceed?')
          
       

         

         

         



}
       
}

}


}
